mixin paginator(data)
    ul.paginator
        li.paginator__item
            a.paginator__link(href="#") 1
        li.paginator__item
            a.paginator__link(href="#") 2
        li.paginator__item
            a.paginator__link(href="#") 3
        li.paginator__item
            span.paginator__link.paginator__link--current 4
        li.paginator__item
            a.paginator__link(href="#") 5
        li.paginator__item
            a.paginator__link(href="#") 6
        li.paginator__item
            a.paginator__link(href="#") 7
        li.paginator__item
            a.paginator__link(href="#") 8
